{
  "name": "smartclass-interview",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tailwind:css && node ./bin/www",
    "tailwind:css": "tailwindcss -c ./assets/tailwind.config.js -i ./assets/css/app.css -o ./public/stylesheets/style.css",
    "tailwind:css:watch": "tailwindcss -c ./assets/tailwind.config.js -i ./src/input.css -o ./dist/output.css --watch"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "follow-redirects": "^1.14.7",
    "http-errors": "~1.6.3",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "tailwindcss": "^3.0.18"
  }
}
